<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="" class="com.demo.demo.Student">
		<property name="id" value="101"></property>
		<property name="name" value="mahajan"></property>
		<property name="dept" value="Dev"></property>
	</bean>
	<bean id="" class="com.demo.demo.Employee">
		<constructor-arg name="id" value="101"></constructor-arg>
		<constructor-arg name="name" value="joe"></constructor-arg>
		<constructor-arg name="dept" value="Hr"></constructor-arg>
	</bean>
	<bean id="e1" class="com.demo.example.Employee">
		<constructor-arg name="id" value="101"></constructor-arg>
		<constructor-arg name="name" value="joe"></constructor-arg>
		<constructor-arg name="dept" value="Hr"></constructor-arg>
		<constructor-arg name="salary" value="12345"></constructor-arg>
	</bean>
	<bean id="o1" class="com.demo.example.Org">
		<property name="id" value="101"></property>
		<property name="name" value="mahajan"></property>
		<property name="loc" value="Dev"></property>
		<property name="employee">
		<ref bean="e1"/>
		</property>
	</bean>
</beans>